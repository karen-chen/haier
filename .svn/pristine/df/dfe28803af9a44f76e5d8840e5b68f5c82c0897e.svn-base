import api from '../api'
import * as types from '../types'

//2 定义初始状态；
const state={
  imgUrl,
}


const getters={}

//3 mutations
const  mutations={
  [types.SB_GET_BANNER](state, res){
    state.imgUrl = res.imgUrl;
  }
}

//4 action
const actions={
  getBanner({commit, state}, req){
    api.getBanner(req, (res)=>{
      commit(types.SB_GET_BANNER, res);
    });
  }
}

export default{
  state,
  actions,
  getters,
  mutations
}
