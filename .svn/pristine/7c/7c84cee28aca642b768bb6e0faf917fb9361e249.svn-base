.seach_body{
  background-color: #f4f4f4;
  height: 100%;
  .el-row{
    padding:pTR(18) pTR(30);
    background-color: #fff;
    &.row_p17{
      @include border(2,#fcfcfc);
    }
    &.row_p0{
      padding: 0;
    }
    .text-right{
      text-align: right;
    }
    .text-center{
      text-align: center;
      .p{
        @include lin_hei(60);
        @include size_color(30,#999);
        width: 100%;
        border: none;
        text-align: center;
        display:block;
        white-space:nowrap;
        overflow:hidden;
        text-overflow:ellipsis;
      }
    }
    .el-button{
      @include size_color(30,#f52b39);
      @include h(60);
      @include lin_hei(60);
      padding: 0;
    }
    .el-input,.inputbox {
      @include h(60);
      background-color: #e6e6e6;
      @include radius(60);
      border: none;
      @include p(0,20);
      position: relative;
    }
    .el-input{
      width: 90%;
    }
    .inputbox{
      .del_btn{
        @include ele-box(40,40);
        @include lin_hei(40);
        background-color: #fff;
        text-align: center;
        @include size_color(40,#e6e6e6);
        font-weight: bold;
        border: none;
        border-radius: 50%;
        position: absolute;
        @include right(20);
        top: 50%;
        transform: translate(0,-50%);
        padding:0;
      }
    }
    .seach_btn{
      @include ele-box(120,60);
      @include lin_hei(60);
      background-color: #f52b39;
      @include size_color(30,#fff);
      border: none;
      @include radius(50);
      padding:0;
    }
    .seach_icon{
      display: inline-block;
      @include ele-box(32,30);
      @include bg(url("../../assets/images/icon_seach.png"));
    }
    p{
      @include size_color(30,#333);
      @include lin_hei(30);
    }
  }
  /*没有搜索到结果的样式*/
  .not_find{
    @include p_t(126);
    @include flex;
    @include flex-center;
    flex-direction: column;
    span{
      display: inline-block;
      @include ele-box(198,129);
      @include bg(url("../../assets/images/icon_uncoupon.png"));
      @include m_b(62);
      &.icon{
        @include ele-box(120,120);
        @include bg(url("../../assets/images/icon_expression.png"));
      }
    }
    p{
      @include size_color(30,#999);
    }
  }
/*搜索到的结果列表*/
  .list_seach{
    padding: 0 pTR(30);
    background-color: #fff;
    ul{width: 100%;
     li{
      padding: pTR(43) 0;
      @include border_b(2,#f5f5f5);
    }
    }
    .el-row{
      padding:pTR(17) 0;
      &.p0{padding: 0;}
    }
    .logo_shop{
      @include ele-box(70,70);
      @include border(2,#d5d5d5);
      overflow: hidden;
    }
    h4{
      @include size_color(24,#999);
    }
    h3{
      @include size_color(30,#333);
    }
    .seach_icon{
      @include ele-box(38,50);
      @include bg(url("../../assets/images/icon_stores.png"));
    }
    .p10{
      padding: 0 pTR(10);
    }
    .pic_icon{
      @include ele-box(30,30);
      @include m_t(0);
    }
    p{
      @include size_color(20,#999);
      @include lin_hei(40);
      width: 100%;
      word-break:break-all;
      word-wrap:break-word;
    }
    }
  }
.list_body{
  background-color: #fff;
  height: 100%;
  .area_roll>div:last-child{display: none;}
  .pic_icon{
    @include ele-box(39,39);
    @include m_t(12);
  }
}


.pop_mask{
  width: 100%;
  height: 100%;
  background-color: rgba(0,0,0,.6);
  position: fixed;
  top:0;
  left: 0;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
  z-index: 99;
  .select_box{
    @include p(30,30);
    background-color: #fff;
    @include radius(10);
    position: relative;
    span.off_btn{
      position: absolute;
      @include ele-box(40,40);
      border-radius: 50%;
      background-color: rgba(0,0,0,3);
      @include border(2,#fff);
      @include right(-10);
      @include top(-10);
      @include size_color(40,#fff);
      text-align: center;
      @include lin_hei(40);
    }
    h2{
      @include size_color(30,#333);
      font-weight: bold;
      text-align: center;
    }
    ul li{
      @include m_b(20);
    }
    .choose{
      display: flex;
      flex-direction: row;
      justify-content: center;
      align-items: center;
      @include m_t(50);
      @include m_b(30);
      select{
        @include ele-box(260,50);
        @include lin_hei(50);
        @include border(1,#eee);
        background-color: #f4f4f4;
        text-align: center;
        @include m(0,20);
        @include p(0,10);
      }
    }
   .check_btn{
     @include ele-box(300,60);
     background-color: #f52b39;
     text-align: center;
     @include size_color(30,#fff);
     @include lin_hei(60);
     @include radius(10);
     margin-left: auto;
     margin-right: auto;
   }

  }
}
.swiper-slide{
  @include h(280);
}
.pop_mask{
  width: 100%;
  height: 100%;
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  .tips{
    @include p(20,40);
    background-color: rgba(51,51,51,.8);
    @include size_color(30,#fff);
    @include m_t(-250);
    position: absolute;
    z-index: 99;
    @include radius(10);
  }
}
