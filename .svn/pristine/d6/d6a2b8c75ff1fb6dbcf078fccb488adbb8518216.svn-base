.seach_body{
  background-color: #f4f4f4;
  height: 100%;
  .el-row{
    padding:pTR(18) pTR(30);
    background-color: #fff;
    &.row_p17{
      @include border(2,#fcfcfc);
    }
    &.row_p0{
      padding: 0;
    }
    .text-right{
      text-align: right;
    }
    .text-center{
      text-align: center;
      .p{
        @include lin_hei(60);
        @include size_color(30,#999);
        width: 77%;
        border: none;
        text-align: center;
      }
    }
    .el-button{
      @include size_color(30,#f52b39);
      @include h(60);
      @include lin_hei(60);
      padding: 0;
    }
    .el-input,.inputbox {
      @include h(60);
      background-color: #e6e6e6;
      @include radius(60);
      border: none;
      @include p(0,20);
      position: relative;
    }
    .el-input{
      width: 80%;
    }
    .inputbox{
      button{
        @include ele-box(120,50);
        @include lin_hei(50);
        background-color: #f52b39;
        @include size_color(30,#fff);
        border: none;
        @include radius(50);
        position: absolute;
        right: 0;
        top: 50%;
        transform: translate(0,-50%);
      }
    }
    .seach_icon{
      display: inline-block;
      @include ele-box(32,30);
      @include bg(url("../../assets/images/icon_seach.png"));
    }
    p{
      @include size_color(30,#333);
      @include lin_hei(30);
    }
  }
  /*没有搜索到结果的样式*/
  .not_find{
    @include p_t(126);
    @include flex;
    @include flex-center;
    flex-direction: column;
    span{
      display: inline-block;
      @include ele-box(120,120);
      @include bg(url("../../assets/images/icon_expression.png"));
      @include m_b(62);
    }
    p{
      @include size_color(30,#999);
    }
  }
/*搜索到的结果列表*/
  .list_seach{
    padding: 0 pTR(30);
    background-color: #fff;
    ul{width: 100%;
     li{
      padding: pTR(43) 0;
      @include border_b(2,#f5f5f5);
    }
    }
    .el-row{
      padding:pTR(17) 0;
      &.p0{padding: 0;}
    }
    .logo_shop{
      @include ele-box(70,70);
      @include border(2,#d5d5d5);
    }
    h4{
      @include size_color(24,#999);
    }
    h3{
      @include size_color(30,#333);
    }
    .seach_icon{
      @include ele-box(38,50);
      @include bg(url("../../assets/images/icon_stores.png"));
    }
    .p10{
      padding: 0 pTR(10);
    }
    .pic_icon{
      @include ele-box(30,30);
      @include m_t(-4);
    }
    p{
      @include size_color(20,#999);
      @include lin_hei(40)
      }
    }
  }
.list_body{
  background-color: #fff;
  height: 100%;
  .area_roll>div:last-child{display: none;}
  .pic_icon{
    @include ele-box(39,39);
    @include m_t(12);
  }
}
