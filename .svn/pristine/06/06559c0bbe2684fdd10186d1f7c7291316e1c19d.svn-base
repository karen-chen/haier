.flow_body{
  height: 100%;
  .el-row{
    @include m(0,32);
    @include p_b(48);
  }
  .el-col{
    @include size_color(36,#333);
    .type{
      @include ele-box(40,40);
      border-radius: 50%;
      @include size_color(30,#fff);
      @include lin_hei(40);
      background-color: #f52b3a;
    }
    h3{
      @include size_color(30,#666);
      @include m_b(45);
    }
    .flow_pic{
      @include w(534);
      height: auto;
      position: relative;
      &.num_pic{
        @include h(266);
        @include bg(url("../../assets/images/flow_bj.jpg"));
      }
      .buy_code{
        @include size_color(52,#fff);
        position: absolute;
        @include top(80);
        left:50%;
        -webkit-transform: translate(-50%,0);
        transform: translate(-50%,0);
        display:inline-block;
        @include w(415);
        background-color: transparent;
        font-weight: bold;
      }
      .copy_btn{
        background-color:#fff;
        @include ele-box(276,60);
        border-color: transparent;
        @include radius(10);
        @include size_color(26,#f52b3a);
        position: absolute;
        @include bottom(40);
        left:50%;
        -webkit-transform: translate(-50%,0);
        transform: translate(-50%,0);
      }
    }
  }
}
