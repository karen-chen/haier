<template>
  <div>
    <div class="bgTop">
      <div class="head">
        <marquee>{{caption}}</marquee>
        <i></i>
      </div>
      <div class="headings"></div>
      <div class="telPic"></div>
      <div class="quanmain">
        <div class="quanTop"></div>
        <div class="box">
          <i></i>
          <p>时间:{{activityTime}}</p>
          <p>地点:{{activityAddress}}</p>
        </div>
        <div class="quanBody">
          <div v-for="item in cardlist" class="quanlist" v-show="havequan">
            <div class="buyTop">
              <div class="nameList">
                <div class="nameLogo"><img :src="imageServerUrlpic+item.card.merchant.merchantLogo"><i>{{item.card.merchant.merchantName}}</i></div>
                <div class="particulars" @click="particulars(item)">特权详情</div>
              </div>
              <div class="partTxt">{{item.card.resourceName}}</div>
            </div>
            <div class="buyBottom">
              <div class="contentTxt">
                <div class="money" v-if=" item.card.configBean.price.value != 0"><span>￥</span><i>{{item.card.configBean.price.value}}</i></div>
                <div class="money" v-if=" item.card.configBean.price.value == 0"><span></span><i></i></div>
                <div class="moneyBtn" @click="showmessRoute(item)" v-if=" item.card.configBean.price.value != 0">购买</div>
                <div class="moneyBtn" @click="showmessRoute(item)" v-if=" item.card.configBean.price.value == 0">立即领取</div>
              </div>
            </div>
          </div>
          <div class="noquan" v-show="noquan"></div>
        </div>
        <div class="quanBottom"></div>
      </div>
    </div>
    <div class="bgBottom">
      <div class="mian">
        <div class="shopping" v-for="title in list" v-show="title.item_list.length !==0">
          <div class="totle">{{title.cat.cat_title}}</div>
          <ul>
            <li class="item" v-for="item in title.item_list" @click="shopDetail(item)">
              <div class="left">
                <img :src="item.item_field4">
              </div>
              <div class="right">
                <h2>{{item.item_title}}</h2>
                <p class="price"><i>￥{{item.item_field2}}</i></p>
                <p class="tunprice">活动价:<span>￥</span><i>{{item.item_field3}}</i></p>
              </div>
            </li>
          </ul>
        </div>
      </div>
    </div>
    <!-- <img @click="showvoucherRoute" class="couponBtn" src="../../assets/czlImg/couponBtn.jpg"> -->
    <!--该活动已经结束-->
    <div class="endBox" v-show="endMess">
      <div class="text">
        <div class="img"></div>
        该活动已关闭
      </div>
    </div>
    <!--详情弹框-->
    <div class="popBox" v-show="showDetail">
      <div class="partBox">
        <div class="mainBox">
          <div class="top">
            <img class="logoPic" :src="imageServerUrlpic+ picUrl">
            <span class="logoName">{{logo}}</span>
            <span class="logoText">{{title}}</span>
          </div>
          <div class="bottom">
            <div class="bottomMain">
              <div class="bottomTitle"></div>
              <div class="tips">
                <h2><i></i>有效日期</h2>
                <p>{{startDate}}至 {{endDate}}</p>
              </div>
              <div class="tips">
                <h2><i></i>使用说明</h2>
                <p class="scrollTop" v-html="description"></p>
              </div>
            </div>
          </div>
          <div class="closePart" @click="particularsClose"></div>
        </div>
      </div>
    </div>
    <!-- 卡券已经停止投放 -->
    <div class="popBox" v-show="Resure">
      <div class="RefundBox">
        <div class="cryno"></div>
        <div class="text">
          卡券已下架，暂时无法购买
        </div>
        <div class="butList">
          <div class="btn2" @click="clickClose">
            确定
          </div>
        </div>
      </div>
    </div>
    <!-- 提示弹框 -->
    <div class="masuccess" v-show="tips">{{masseg}}</div>
    <!-- 验证码 -->
    <div class="popmask" v-show="verificationcode">
      <div class="verificationcode">
        <div class="input_box"><input placeholder="请输入4位验证码完成领取" v-model="inputCode" maxlength="4"></div>
        <p>请向销售员索要验证码</p>
        <div class="sureBtn" @click="submitSure">确认</div>
        <div class="closeSure" @click="closeSure"></div>
      </div>
    </div>
  </div>
</template>
<script src="http://res.wx.qq.com/open/js/jweixin-1.2.0.js"></script>
<script>
import * as config from '@/lib/config'
import {
  userData,
  EnumConfigType,
  ShareInfo,
  getJsSign,
  wxconfig,
  wxShare,
  wxHideMenu
} from '@/lib/tools';


export default {
  data() {
    return {
      caption: '',
      activityAddress: '',
      activityTime: '',
      list: [],
      shareTitle: '',
      title: '',
      shareImg: '',
      shareUrl: '',
      shareDesc: '',
      endMess: false,
      documentTitle: '',
      cardlist: [],
      imageServerUrlpic: config.imageServerUrl,
      nameLogo: '',
      logo: '',
      allow: true,
      status: 0,
      money: 0,
      money1: 0,
      canCustomerInfo: '',
      recognizeCardSid: '',
      recognizeCardSid1: '',
      onoff: true,
      cardNo: '',
      picUrl: '',
      showDetail: false,
      startDate: '',
      endDate: '',
      description: '',
      canCustomerInfo: '',
      recognizeCardSid: '',
      Resure: false,
      tips: false,
      masseg: '',
      noquan: false,
      havequan: true,
      verificationcode: false,
      vcodesid: '',
      inputCode: '',
      pageHash:'',
      submitSureIndex:true
    }
  },

  created() {
    console.log(1111111111,userData.hash);
    if (typeof WeixinJSBridge === "undefined") {
      if (document.addEventListener) {
        document.addEventListener('WeixinJSBridgeReady', this.onBridgeReady, false);
      } else if (document.attachEvent) {
        document.attachEvent('WeixinJSBridgeReady', this.onBridgeReady);
        document.attachEvent('onWeixinJSBridgeReady', this.onBridgeReady);
      }
    } else {
      this.onBridgeReady();
    }


    var self = this;

    //分享功能
    getJsSign(function(sign) {
      wxconfig(sign);
    })
    wx.ready(function() {
      wxHideMenu();
      wx.showMenuItems({
        menuList: [
          'menuItem:share:qq', //分享到QQ
          'menuItem:share:weiboApp', //分享到Weibo
          'menuItem:favorite', //收藏
          'menuItem:share:facebook', //分享到facebook
          'menuItem:share:QZone', //分享到QQ空间
          "menuItem:share:timeline", //分享到朋友圈
          //保护类
          "menuItem:share:appMessage", //发送给朋友
          'menuItem:editTag', //编辑标签
          'menuItem:delete', //删除
          'menuItem:copyUrl', //复制链接
          'menuItem:originPage', //原网页
          'menuItem:readMode', //阅读模式
          'menuItem:share:email', //邮件
          'menuItem:share:brand' //一些特殊公众号
        ] // 要显示的菜单项，所有menu项见附录3
      });

    });

    self.pageHash = window.location.hash.substr(2);
    //判断活动是否有效
    $.ajax({
      url: config.locationUrl + "/activity/checkActivityIng",
      dataType: "jsonp",
      data: {
        actSid: userData.actSid,
      },
      success: function(data) {
        if (data.ret === "0") {

          //获取页面配置信息
          self.getBaseConfig();
          self.getProductModuleList();
          self.getRecognizeCardList();

          $.ajax({
            url: config.locationUrl + "/visitor_log/addVisitorLog",
            dataType: "jsonp",
            data: {
              actSid: userData.actSid,
              saleOpenid: userData.saleOpenid,
              openid:userData.openid,
              sourcePage:self.pageHash,
              actionType:1
            },
            success: function(data) {

            }
          });


        } else {
          //处理接口调用失败
          self.endMess = true;
        }
      },
      error(res) {
        self.error('checkActivityIng');
      }
    });
    // console.log(5555555, this.activityAddress);
  },

  methods: {
    error(msg) {
      console.log('msg:', 'fail');
    },

    showvoucherRoute() {
      this.$router.push({
        name: 'Quan'
      })
    },

    //获取分享基本信息以及相关权限
    getBaseConfig() {
      var self = this;
      $.ajax({
        url: config.locationUrl + "/sys/getBaseConfigByType",
        dataType: "jsonp",
        data: {
          actSid: userData.actSid,
          configType: EnumConfigType.RECOGNIZE
        },
        success: function(data) {
          // console.log(11111, data);
          if (data.ret === "0") {

            self.activityAddress = data.activityAddress;

            self.activityTime = data.activityTime;
            self.caption = data.caption;
            if (userData.saleOpenid) {
              config.wxShareData.link = config.cdnUrl + '/index.html?actSid=' + userData.actSid + '&saleOpenid=' + userData.saleOpenid + '#/cardmain';
            } else {
              config.wxShareData.link = config.cdnUrl + '/index.html?actSid=' + userData.actSid + '#/cardmain';
            }

            // config.wxShareData.link = config.cdnUrl + 'http://1251097942.cdn.myqcloud.com/1251097942/haier/test/index.html#/shoplist';
            // console.log(self.activityTime);
            //文章标题
            if (data.title) {
              document.title = data.title;
              self.documentTitle = data.title;

            } else {
              document.title = '认筹活动详情';
              self.documentTitle = '认筹活动详情';
            }
            //分享标题
            if (data.shareTitle) {
              config.wxShareData.title = data.shareTitle;
            } else {
              config.wxShareData.title = '超值认筹劵，再不来领就没有了!';
            }

            //分享详情
            if (data.shareDesc) {
              config.wxShareData.desc = data.shareDesc;
            } else {
              config.wxShareData.desc = '超值认筹劵，再不来领就没有了!';
            }

            //分享图片
            if (data.shareImg) {
              config.wxShareData.imgUrl = config.imageServerUrl + data.shareImg;
            } else {
              config.wxShareData.imgUrl = config.cdnUrl + '/static/share/share5.jpg';
            }
            wxShare();
            // console.log(self.shareImg);


          } else {
            document.title = '认筹活动详情';
            config.wxShareData.title = '超值认筹劵，再不来领就没有了!';
            config.wxShareData.desc = '超值认筹劵，再不来领就没有了!';
            config.wxShareData.imgUrl = config.cdnUrl + '/static/share/share5.jpg';
            if (userData.saleOpenid) {
              config.wxShareData.link = config.cdnUrl + '/index.html?actSid=' + userData.actSid + '&saleOpenid=' + userData.saleOpenid + '#/cardmain';
            } else {
              config.wxShareData.link = config.cdnUrl + '/index.html?actSid=' + userData.actSid + '#/cardmain';
            }
            wxShare();
            //处理接口调用失败
            self.error('getBaseConfigByType');
          }
        },
        error(res) {
          self.error('getBaseConfigByType');
        }
      });
    },

    // banner/getProductModuleList 获取产品信息

    getProductModuleList() {
      var self = this;
      $.ajax({
        url: config.locationUrl + "/product/list_act_page_cat_item",
        dataType: "jsonp",
        data: {
          act_sid: userData.actSid
        },
        success: function(data) {
          console.log("33333", data);
          if (data.ret === "0") {
            data.data = data.data || [];
            data.data.forEach(obj => {
              obj.item_list = obj.item_list || [];
              obj.item_list.forEach(item => {
                item.item_field4 = config.imageServerUrl + item.item_field4;
              });
            });
            self.list = data.data;
            // console.log(data.list);
          } else {
            //处理接口调用失败
            self.error('getBannerList');
          }
        },
        error(res) {
          self.error('getBannerList');
        }
      });
    },

    //获取券列表
    getRecognizeCardList() {
      var self = this;
      $.ajax({
        url: config.locationUrl + "/activity/getRecognizeCardList",
        dataType: "jsonp",
        data: {
          actSid: userData.actSid,
          openid: userData.openid
        },
        success: function(data) {
          console.log(6666666, data);
          if (data.ret === "0") {

            if (data.list.length == 0) {
              //没有券可以购买 显示没有购买信息
              self.noquan = true;
              self.havequan = false;
            } else {
              //有券购买,显示列表
              self.noquan = false;
              self.havequan = true;
              self.cardlist = data.list;
              console.log(55555555,self.cardlist);
            }
            // console.log(self.endMess);
            //返回值为-88 表示活动已经结束
          }
          if (data.ret === "-88") {
            self.endMess = true;
          } else {
            //处理接口调用失败
            self.error('getRecognizeCardList');
          }
        },
        error(res) {
          self.error('getRecognizeCardList');
        }
      });
    },
    //点击购买按钮判断
    showmessRoute(item) {
      // localStorage.setItem('recognizeCardSid', item.sid);
      // console.log(item.card.configBean.price.value);
      var self = this;
      if (!self.allow) {
        return;
      }
      self.allow = false;
      setTimeout(() => {
        self.allow = true;
      }, 500)
      self.money = Math.round(item.card.configBean.price.value * 100);
      self.recognizeCardSid = item.sid;
      self.canCustomerInfo = item.card.configBean.canCustomerInfo.value;
      localStorage.setItem('recognizeCardSid', self.recognizeCardSid);
      localStorage.setItem('money', self.money);
      $.ajax({
        url: config.locationUrl + "/activity/getRecognizeCardDetial",
        dataType: "jsonp",
        data: {
          recognizeCardSid: self.recognizeCardSid,
          openid: userData.openid
        },
        success: function(data) {
          console.log(1111111222222,data.obj.card.configBean.canSaleCode);
          if (data.ret === "0") {

            self.status = parseInt(data.ret);
            self.cardNo = data.obj.card.sid;
            localStorage.setItem('checkMember', self.cardNo);

            if (data.obj.card.configBean.canSaleCode == null || data.obj.card.configBean.canSaleCode.value == '0') {
              self.wx_pay();

            } else {
              self.verificationcode = true;
              $('.body').css('overflow','hidden');
            }

          }
          if (data.ret === "-1") {
            self.Resure = true;
            self.tipsShow('卡券已下架');
            return;
          }
          if (data.ret === "2") {
            self.tipsShow('卡券已过期');
            return;
          }
          if (data.ret === "3") {
            self.tipsShow('库存不够');
            return;
          }
          if (data.ret === "4") {
            self.tipsShow('购买数量已达上限');
            return;
          }
          if (data.ret === "-88") {
            self.endMess = true;
          }
        },
        error(res) {
          self.error('getRecognizeCardList');
        }
      });


      // this.$router.push({
      //   name: 'Buy'
      // })
    },
    onBridgeReady() {
      console.log("组件加载完毕");
    },
    //弹框定时显示
    tipsShow(meg) {
      var self = this;
      self.tips = true;
      self.masseg = meg;
      setTimeout(() => {
        self.tips = false;
      }, 600);
    },
    wx_pay() {
      var self = this;
      $.ajax({
        url: config.locationUrl + "/wx_pay/pay_sign",
        dataType: "jsonp",
        type: "post",
        data: {
          openid: userData.openid,
          money: self.money,
          cardNo: self.cardNo,
          recognizeCardSid: self.recognizeCardSid,
          actId: userData.actSid,
          saleOpenid: userData.saleOpenid,
          codeSid: self.vcodesid
        },
        success: function(data) {

          if (data.ret === "0") {
            //订单ID
            localStorage.setItem('orderId', data.order.sid);
            if (self.money == 0) {
              if (self.canCustomerInfo === '1') {
                self.$router.push({
                  name: 'Submit'
                })
              } else {
                self.$router.push({
                  name: 'Buysuccess'
                })
              }
              return
            } else {

              self.invokePayDlg(data);
            }
            // self.sign = data.sign;

          } else {
            //处理接口调用失败
            self.error('getCustomData');
          }
        },
        error(res) {
          self.error('getCustomData');
        }
      });
    },

    invokePayDlg(ret) {
      var self = this;
      WeixinJSBridge.invoke(
        'getBrandWCPayRequest',
        ret.sign,
        function(res) {
          //alert(res.err_msg);
          // $("#log_info").append("<p>支付结果："+res.err_msg+"</p>");
          // 使用以上方式判断前端返回,微信团队郑重提示：res.err_msg将在用户支付成功后返回    ok，但并不保证它绝对可靠。
          if (res.err_msg == "get_brand_wcpay_request:ok") {
            //CT：实际是否成交以后台接口为准
            // $("#out_trade_no").val(now_time);
            if (self.canCustomerInfo === '1') {
              self.$router.push({
                name: 'Submit'
              })
            } else {
              self.$router.push({
                name: 'Buysuccess'
              })
            }

          } else {
            //其他情况：get_brand_wcpay_request:cancel或者get_brand_wcpay_request:fail，后者在配置错误时也会出现，日
          }
        }
      );
    },
    particulars(item) {
      this.showDetail = true;
      var self = this;
      self.recognizeCardSid1 = item.sid;
      $.ajax({
        url: config.locationUrl + "/activity/getRecognizeCardDetial",
        dataType: "jsonp",
        data: {
          recognizeCardSid: self.recognizeCardSid1,
          openid: userData.openid
        },
        success: function(data) {
          console.log(data);
          if (data.ret === "0") {
            self.status = parseInt(data.ret);
            // self.buyBtn = data.obj.card.configBean.price.value + '元购买';
          }
          self.obj = data.obj;
          // self.money1 = Math.round(data.obj.card.configBean.price.value * 100);
          self.picUrl = data.obj.card.merchant.merchantLogo;
          self.logo = data.obj.card.merchant.merchantName;
          self.title = data.obj.card.resourceName;
          self.startDate = data.obj.card.configBean.startDate.value;
          self.endDate = data.obj.card.configBean.endDate.value;
          self.description = "<p>" + data.obj.card.configBean.description.value.replace(/\n/g, "</p><p>") + "</p>";
          self.canCustomerInfo = data.obj.card.configBean.canCustomerInfo.value;
          self.cardNo = data.obj.card.sid;
          //卡券ID
          localStorage.setItem('checkMember', self.cardNo);
          localStorage.setItem('money', self.money);
        },
        error(res) {
          self.error('getRecognizeCardList');
        }
      });
    },
    particularsClose() {
      this.showDetail = false;
    },
    clickClose() {
      this.Resure = false;
      this.getRecognizeCardList();
    },

    //商品列表跳转详情
    shopDetail(item) {

      localStorage.setItem('page_item_sid', item.sid);
      if (item.img_count > 0) {
        this.$router.push({
          name: 'Shopdetail'
        })
      }
      // console.log(item.sid)

    },
    submitSure() {
      var self = this;
      $('.body').css('overflow','auto');
      console.log(this.inputCode);
      if(!self.submitSureIndex){
        console.log(12121211111111)
        return
      }
      self.submitSureIndex = false
      console.log('hhhhhhhhh')
      $.ajax({
        url: config.locationUrl + "/code/useCode",
        dataType: "jsonp",
        data: {
          code: this.inputCode
        },
        success: function(data) {
          if (data.ret === "0") {
            self.vcodesid = data.data.sid;
            self.wx_pay();
          } else {
            console.log(111111111);
            self.tipsShow('验证码错误');
          }
          self.submitSureIndex = true
        },

      });
    },
    closeSure() {
      this.verificationcode = false;
     /* this.submitSureIndex = false*/
      $('.body').css('overflow','auto');
    }
  }
}

</script>
